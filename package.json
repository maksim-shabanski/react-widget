    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.1",
    "style-loader": "^1.1.2",
    "stylelint": "^12.0.1",
    "stylelint-config-hudochenkov": "^3.0.1",
